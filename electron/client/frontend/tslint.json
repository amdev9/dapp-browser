{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "no-trailing-whitespace": false,
        "no-empty-interface": false,
        "no-var-requires": false,
        "ordered-imports": false,
        "member-ordering": false,
        "interface-name": false,
        "no-namespace": false,
        "no-console": false,
        "semicolon": [
            true,
            "never"
        ]
    },
    "rulesDirectory": []
}